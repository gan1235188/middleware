{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        //最大行数
        "max-line-length": {
            "options": [200]
        },
        "quotemark": [
            true,
            "single"
        ],
        "no-implicit-dependencies":[true,["src"]],
        "no-submodule-imports":[true,"src"],
    },
    "rulesDirectory": []
}